{
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "printWidth": 120,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "none",
    "useTabs": false,
    "bracketSameLine": false,
    "plugins": [
        "prettier-plugin-organize-attributes"
    ],
    "attributeGroups": [
        "^repeat.for$",
        "^if.bind$",
        "^click.delegate$",
        "$DEFAULT",
        "^data-cy"
    ]
}